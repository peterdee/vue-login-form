{
  "name": "vue-login-form",
  "version": "0.0.2",
  "author": "Peter Dyumin",
  "description": "Login form with Vue",
  "private": true,
  "license": "ISC",
  "main": "src/main.js",
  "scripts": {
    "all": "NODE_ENV='production' && vue-cli-service build && node ./server.js",
    "build": "vue-cli-service build",
    "client": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "server": "nodemon ./server.js"
  },
  "engines": {
    "node": "13.1.0",
    "npm": "6.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterdee/vue-login-form"
  },
  "keywords": [
    "Vue",
    "Login form"
  ],
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.3.2",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
